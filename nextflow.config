manifest {
    mainScript = 'phage.nf'
}

// default parameters
params {
  cores = "8"
  mem = "12"
  help = false

  // folder structure
  output = 'results'
}


profiles {

  standard {
        process.executor = 'local'
        docker { enabled = true }
        params.cpus = params.cores
        process {
            withName:dummy { container = 'dockerpath' }
} } }
